                            "default": ""
                        },
                        "mail2": {
                            "$id": "#/properties/data/items/anyOf/0/properties/mail2",
                            "type": "string",
                            "title": "mail2 schema",
                            "description": "Indirizzo email associato all'Unità Organizzativa di tipo PEC|CECPAC|ALTRO",
                            "default": ""
                        },
                        "mail3": {
                            "$id": "#/properties/data/items/anyOf/0/properties/mail3",
                            "type": "string",
                            "title": "mail3 schema",
                            "description": "Indirizzo email associato all'Unità Organizzativa di tipo PEC|CECPAC|ALTRO",
                            "default": ""
                        },
                        "nome_resp": {
                            "$id": "#/properties/data/items/anyOf/0/properties/nome_resp",
                            "type": ["string", "null"],
                            "title": "nome_resp schema",
                            "description": "Nome del responsabile dell'Unità Organizzativa",
                            "default": null
                        },
                        "cogn_resp": {
                            "$id": "#/properties/data/items/anyOf/0/properties/cogn_resp",
                            "type": ["string", "null"],
                            "title": "cogn_resp schema",
                            "description": "Cognome del responsabile dell'Unità Organizzativa",
                            "default": null
                        },
                        "mail_resp": {
                            "$id": "#/properties/data/items/anyOf/0/properties/mail_resp",
                            "type": ["string", "null"],
                            "title": "mail_resp schema",
                            "description": "Indirizzo email del responsabile dell'Unità Organizzativa",
                            "default": null
                        },
                        "tel_resp": {
                            "$id": "#/properties/data/items/anyOf/0/properties/tel_resp",
                            "type": ["string", "null"],
                            "title": "tel_resp schema",
                            "description": "Numero di telefono del responsabile dell'Unità Organizzativa",
                            "default": null
